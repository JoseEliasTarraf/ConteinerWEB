@model IEnumerable<Conteiner>

<h1>Clientes</h1>

@using(Html.BeginForm("Search","Search",FormMethod.Get)){
    @Html.TextBox("searching")
    <input type="submit" value="searching"/>
}
<table class="table table-striped">
    <thead>
        <tr>
            <td>Cliente</td>
            <td>Numero</td>
            <td>Tipo</td>
            <td>Status</td>
            <td>Categoria</td>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="" style="color:red">
                    Cliente não encontrado
                </td>
            </tr>
        }
        else
        {
            foreach (var cl in Model)
            {
                <tr>
                    <td>@cl.Cliente</td>
                    <td>@cl.Numero</td>
                    <td>@cl.Tipo</td>
                    <td>@cl.Status</td>
                    <td>@cl.Categoria</td>
                </tr>
            }
        }
    </tbody>
</table>
<a class="btn btn-primary btn-sm" href="/Search/SearchMovi?id>SearchMovi">Movimentações</a>