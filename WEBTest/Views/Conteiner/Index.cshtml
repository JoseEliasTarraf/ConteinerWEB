@model IEnumerable<Conteiner>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Cliente</th>
            <th scope="col">Numero</th>
            <th scope="col">Tipo</th>
            <th scope="col">Status</th>
            <th scope="col">Categoria</th>
        </tr>
    </thead>
    <tbody>
        @if(Model != null)
        {
            @foreach(var cl in Model)
            {
                <tr>
                    <th scope="row">@cl.Id</th>
                    <td>@cl.Cliente</td>
                    <td>@cl.Numero</td>
                    <td>@cl.Tipo</td>
                    <td>@cl.Status</td>
                    <td>@cl.Categoria</td>
                </tr>
            }
        }
    </tbody>
</table>
<a class="btn btn-primary btn-sm" href="/Conteiner/Add?id>Add">Adicionar</a>
<a class="btn btn-primary btn-sm" href="/Moviment/Index?index>Index">Movimentações</a>